{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_OPTIONS=--experimental-json-modules nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@adminjs/express": "5.1.0",
    "@adminjs/nestjs": "6.1.0",
    "@adminjs/prisma": "5.0.3",
    "@getbrevo/brevo": "^2.2.0",
    "@nestjs/bull-shared": "10.2.0",
    "@nestjs/bullmq": "10.2.0",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "10.3.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.10",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/schedule": "4.1.2",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/throttler": "6.4.0",
    "@prisma/client": "5.17.0",
    "@sentry/nestjs": "^8.55.0",
    "adminjs": "7.8.16",
    "axios": "^1.7.2",
    "bullmq": "^5.12.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.3.0",
    "cookie-parser": "^1.4.6",
    "express": "4.21.2",
    "express-formidable": "^1.2.0",
    "helmet": "^7.1.0",
    "nestjs-pino": "^4.1.0",
    "pino-http": "^10.2.0",
    "pino-pretty": "^11.2.1",
    "prom-client": "^15.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.2",
    "@nestjs/testing": "10.3.10",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "^20.14.12",
    "@types/supertest": "^6.0.2",
    "jest": "29.7.0",
    "prisma": "5.17.0",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.3",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "overrides": {
    "@tiptap/core": "2.4.0",
    "@tiptap/extension-collaboration": "2.4.0",
    "@tiptap/extension-collaboration-cursor": "2.4.0",
    "@tiptap/extension-horizontal-rule": "2.4.0",
    "@tiptap/pm": "2.4.0",
    "@tiptap/react": "2.4.0",
    "@tiptap/starter-kit": "2.4.0"
  }
}
